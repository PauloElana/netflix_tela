{
  "expo": {
    "name": "KFLIX",
    "slug": "KFLIX",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#111111"
    },
    "platforms": ["ios", "android"],
    "statusBar": {
      "style": "light",
      "backgroundColor": "#000000"
    },
    "ios": {
      "bundleIdentifier": "com.yourname.kflix",
      "buildNumber": "1.0.0",
      "supportsTablet": true
    },
    "android": {
      "package": "com.yourname.kflix",
      "versionCode": 1,
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#000000"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
